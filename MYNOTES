--SELECT SORGULARINDA WHERE KULLANIMI 
 SELECT* FROM PERSONELLER WHERE 'SEHIR=LONDON'
 SELECT*FROM PERSONELLER WHERE BaglıCalıstıgıKısı <5
   --AND , OR OPERETÖRLERİ
   SELECT*FROM PERSONELLER WHERE SEHIR ='LONDON' AND ULKE ='UK'
   SELECT*FROM PERSONELLER WHERE UNVANEKİ='MR'OR SEHİR='NYC'
